    "kalend-layout": "0.0.9",
    "lodash": "4.17.21",
    "luxon": "2.3.0",
    "morgan": "1.10.0",
    "nodemailer": "6.7.2",
    "otplib": "12.0.1",
    "pg": "8.7.1",
    "pretty-quick": "2.0.2",
    "redis": "3.1.2",
    "rrule": "2.6.8",
    "socket.io": "4.4.1",
    "tsconfig-paths": "3.12.0",
    "tsdav": "2.0.1",
    "typeorm": "0.2.43",
    "uuid": "8.3.2",
    "web-push": "3.4.5",
    "winston": "3.6.0",
    "winston-daily-rotate-file": "4.6.1"
  }
}
