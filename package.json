{
  "name": "bloben-api",
  "version": "0.1.0",
  "description": "",
  "baseUrl": "./",
  "license": "AGPL",
  "author": "nibdo",
  "main": "build/index.js",
  "directories": {
    "test": "src/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nibdo/bloben-api"
  },
  "scripts": {
    "prepare": "npx husky install && npx husky add .husky/pre-commit \"npm run lint\"",
    "start": "tsc && node build/index.js",
    "build": "tsc -b",
    "typeorm-cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config build/config/ormconfig.js",
    "migration": "typeorm-cli migration:run",
    "dev": "nodemon -L -e ts,json --exec 'node --require ts-node/register src/index.ts'\n",
    "debug": "nodemon --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register src/index.ts'\n",
    "test-pipeline": "NODE_ENV=test REDIS_HOST=localhost REDIS_PORT=6379 DB_PORT=5432 DB_HOST=localhost DB_CONNECTION=postgres DB_SYNCHRONIZE=true DB_ENTITIES=src/data/entity/*.ts DB_ENTITIES_DIR=../../src/data/entity/*.ts DB_USERNAME=bloben_test DB_PASSWORD=bloben_test DB_DATABASE=bloben_test ts-mocha --config ./mocharc.js",
    "test": "./node_modules/.bin/env-cmd -f ./.env.test.local ts-mocha --config ./mocharc.js",
    "docker:test:up": "docker-compose -f docker-compose.test-local.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test-local.yml down",
    "lint": "eslint . --ext ts"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/lodash": "4.14.178",
    "@types/luxon": "2.0.9",
    "@types/mocha": "9.1.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "chai": "4.3.6",
    "env-cmd": "10.1.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "mocha": "9.2.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rewiremock": "3.14.3",
    "sinon": "13.0.1",
    "supertest": "6.1.6",
    "ts-mocha": "9.0.2",
    "ts-mock-imports": "1.3.8",
    "ts-node": "10.5.0",
    "ts-node-dev": "2.0.0-0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "async-redis": "2.0.0",
    "axios": "0.26.0",
    "bcrypt": "5.0.1",
    "body-parser": "1.19.2",
    "bullmq": "1.74.0",
    "connect-redis": "5.2.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cron": "1.8.2",
    "crypto-js": "4.1.1",
    "debug": "3.2.7",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-session": "1.17.2",
    "helmet": "5.0.2",
    "http-errors": "2.0.0",
    "http-proxy-middleware": "2.0.3",
    "ical-js-parser-commonjs": "0.4.0",
    "joi": "17.6.0",
